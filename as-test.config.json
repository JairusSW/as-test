{
  "$schema": "./as-test.config.schema.json",
  "input": ["./assembly/__tests__/*.spec.ts"],
  "outDir": "./.as-test/build",
  "logs": "./.as-test/logs",
  "snapshotDir": "./.as-test/snapshots",
  "config": "none",
  "coverage": true,
  "buildOptions": {
    "args": [],
    "target": "bindings"
  },
  "runOptions": {
    "runtime": {
      "name": "node",
      "run": "node ./tests/<name>.run.js"
    }
  }
}
